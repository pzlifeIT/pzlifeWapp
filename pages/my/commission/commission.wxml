<view class="bg">
    <image lazy-load="true" src="{{imgHost}}bg.png"></image>
    <view class="balance-text">佣金余额</view>
    <view class="balance-num">{{commission.commission || 0}}
        <text>元</text>
    </view>
    <view class="three" >
        <view class="three-total">佣金总额
            <text>(元)</text>
            <view>{{commission.commission_all || 0}}</view>
        </view>
        <view class="three-total">已提现
            <text>(元)</text>
            <view>{{commission.commission_extract || 0}}</view>
        </view>
        <view class="three-used">已转商票
            <text>(元)</text>
            <view>{{commission.commission_to_balance || 0}}</view>
        </view>
    </view>
</view>
