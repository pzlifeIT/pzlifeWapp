/* pages/my/activeEarn/activeEarn.wxss */
page{
    background-color: #f7f7f7;
}
.top{
    width: 750rpx;
    height: 260rpx;
    background-color: #f99f36;
    position: fixed;
    top: 0;
    display: flex;
    justify-content: space-between;
    align-items: center;
}
.left{
    width: 374rpx;
    height: 100%;
    text-align: center;
}
.text{
    color: #FFFFFF;
    font-size: 26rpx;
    margin-top: 80rpx;
}
.num{
    color: #FFFFFF;
    font-size: 60rpx;
}
.mid{
    width: 2rpx;
    height: 76rpx;
    background-color: #FFFFFF;
}
.text-left::after{
    content: " (个)";
    font-size: 22rpx;
}
.text-right::after{
    content: ' (元)';
    font-size: 22rpx;
}
.detail-text{
    width: 750rpx;
    height: 70rpx;
    text-align: center;
    line-height: 80rpx;
    color: #000000;
    font-size: 28rpx;
    margin-top: 260rpx;
}
.one{
    width: 750rpx;
    height: 154rpx;
    background-color: #FFFFFF;
    margin-bottom: 2rpx;
    padding: 0 16rpx 0 10rpx;
    box-sizing: border-box;
    display: flex;
    /*justify-content: space-between;*/
    align-items: center;
}
.avatar{
    width: 116rpx;
    height: 100%;
    /*flex-grow: 1;*/
    margin-right: 16rpx;
}
.avatar image{
    width: 96rpx;
    height: 96rpx;
    background: url("data:image/png; base64,");
    background-size: 96rpx 96rpx;
    margin: 29rpx 0 0 10rpx;
    border-radius: 50%;
    border: 1rpx solid #f7f7f7;
}
text{
    display: inline-block;
}
.name-time{
    text-align: left;
    flex-grow: 4;
    /*display: flex;*/
    /*justify-content: start;*/
    /*align-items: center;*/
}
.name{
    color: #666666;
    font-size: 26rpx;
    width: 510rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
    margin-bottom: 16rpx;
}
.time{
    font-size: 28rpx;
    color: #666666;
}
.money{
    flex-grow: 1;
    height: 100%;
    text-align: center;
    line-height: 154rpx;
    color: #e61f18;
    font-size: 32rpx;
}
.money::after{
    content: '元';
    font-size: 22rpx;
}
.symbol{
    font-size: 22rpx;
}
.null{
    width: 750rpx;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
    margin-top: 504rpx;
}
.center{
    width: 360rpx;
    height: auto;
    overflow: auto;
    display: flex;
    align-items: center;
    flex-direction: column;
}
.center image{
    width: 219rpx;
    height: 200rpx;
    margin-bottom: 48rpx;
}
.null-text{
    color: #d6d6d6;
    font-size: 28rpx;
    width: 100%;
    text-align: center;
}