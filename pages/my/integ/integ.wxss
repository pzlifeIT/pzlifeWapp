/* pages/my/case/case.wxss */
page{
	height: 100%;
	background: #F7F7F7;
	/* overflow: hidden; */
}
.balance{
	width: 750rpx;
	height: 240rpx;
	background-image: url(data:image/png;base64,);
	background-repeat: none;
	background-size: 750rpx 240rpx;
	overflow: hidden;
}
.balance-text{
	width: 750rpx;
	overflow: hidden;
	text-align: center;
	font-size: 28rpx;
	color: #fff;
	margin-top: 38rpx;
}
.balance-num{
	width: 750rpx;
	overflow: hidden;
	text-align: center;
	font-size: 60rpx;
	color: #fff;
	margin-top: 10rpx;
}
.balance-num text{
	font-size: 28rpx;
	display: inline-block;
	margin-left:11rpx ;
}
.detail{
	width: 750rpx;
	overflow: hidden;
}
.detail-head{
	background: #F7F7F7;
	height: 68rpx;
	width: 750rpx;
	text-align: center;
	line-height: 68rpx;
	color: #202020;
	font-size: 32rpx;
}
::-webkit-scrollbar {
    width: 0px;
    height: 0px;
    color: transparent;
}

::-webkit-scrollbar-track {
    box-shadow: inset 0 0 10px rgba(0, 0, 0, 0.3);
    border-radius: 10px;
    background-color: #f7f7f7;
}
.detail-one{
	width: 750rpx;
	height: 180rpx;
	border-bottom: 1rpx solid #F7F7F7;
	background: #fff;
}
.list-left{
	width: 70%;
	height: 150rpx;
	margin-left: 12rpx;
	padding-top: 30rpx;
	float: left;
}
.list-left .title{
	color: #202020;
	font-size: 30rpx;
	margin-bottom: 12rpx;
}
.list-left .order{
	color: #666;
	font-size: 24rpx;
	margin-bottom: 12rpx;
}
.list-left .time{
	color: #666;
	font-size: 24rpx;
}
.list-right{
	width: 20%;
	height: 180rpx;
	float: right;
	color: #202020;
	font-size: 28rpx;
	text-align: center;
	line-height: 180rpx;
}
.active{
	color: #E61F18;
}
.not{
	width: 750rpx;
	height: 70%;
	overflow: hidden;
	display: flex;
	justify-content: center;	
	align-items: center;
}
.not-case{
	width: 156rpx;
	overflow: hidden;
}
.not image{
	width: 156rpx;
	height: 200rpx;
	margin-bottom: 29rpx;
}
.not .text{
	width: 156rpx;
	overflow: hidden;
	text-align: center;
	color: #D6D6D6;
	font-size: 28rpx;
}














