/* pages/comfirOrder/comfirOrder.wxss */

page {
    position: relative;
    background-color: #f7f7f7;
    width: 100%;
    height: 100%;
    overflow: auto;
}

.addres {
    margin-top: 2rpx;
    width: 731rpx;
    height: auto;
    position: relative;
    padding: 37rpx 10rpx 20rpx 9rpx;
    background: #ffffff;
}

.addres::after {
    background-image: url("http://pnkp5i8sb.bkt.clouddn.com/add.png");
    /* content: "123"; */
}

.addres_outer {
    display: inline-block;
    width: 700rpx;
}

.addres .name_phone {
    font-size: 30rpx;
    color: #404040;
}

.name_phone .name {
    margin-right: 34rpx;
}

.addres_detail .default {
    font-size: 24rpx;
    color: #e61f18;
    display: inline-block;
    width: 62rpx;
    height: 30rpx;
    border-radius: 2px;
    border: 1rpx solid #e61f18;
    text-align: center;
    line-height: 30rpx;
    margin-right: 34rpx;
}

.addres_detail .location {
    font-size: 26rpx;
    color: #888;
}

.jt {
    width: 16rpx;
    height: 30rpx;
    position: absolute;
    right: 10rpx;
    top: 35%;
}

.jt image {
    width: 100%;
    height: 100%;
}

.blue_red {
    height: 6rpx;
    width: 750rpx;
    position: absolute;
    left: 0;
    bottom: 0;
}

.goods {
    margin-top: 10rpx;
}

.goods_info {
    width: 750rpx;
    height: 216rpx;
    background-color: #ffffff;
    margin-bottom: 1rpx;
    position: relative;
}

.goods_img {
    width: 172rpx;
    height: 172rpx;
    display: inline-block;
    margin-top: 22rpx;
    margin-left: 10rpx;
}

.goods_img image {
    width: 100%;
    height: 100%;
    background: paleturquoise;
}

.goods_detail {
    display: inline-block;
    width: 470rpx;
    height: 176rpx;
    margin-left: 19rpx;
    vertical-align: top;
    padding-top: 40rpx;
}

.goods_detail .goods_title {
    font-size: 30rpx;
    color: #202020;
    display: block;
    margin-bottom: 17rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.goods_detail .goods_subtitle {
    font-size: 24rpx;
    color: #888888;
    display: block;
    margin-bottom: 25rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    white-space: nowrap;
}

.goods_detail .price {
    font-size: 30rpx;
    color: #e61f18;
}

.sum {
    position: absolute;
    right: 10rpx;
    top: 40%;
    font-size: 28rpx;
    color: #404040;
}

.stat {
    width: 750rpx;
    position: relative;
    padding-bottom: 290rpx;
}

.stat .summation {
    width: 750rpx;
    height: 80rpx;
    margin-top: 1rpx;
    color: #404040;
    font-size: 30rpx;
    line-height: 80rpx;
    position: relative;
    background-color: #fff;
    overflow: hidden;
}

.stat .rebate {
    height: 120rpx;
}

.sum_text {
    float: left;
    margin-left: 10rpx;
}

.sum_price {
    float: right;
    margin-right: 12rpx;
}

.small {
    width: 100%;
    margin-top: -10rpx;
    line-height: 40rpx;
    text-indent: 10rpx;
    font-size: 26rpx;
    color: #999;
    color: #e61f18;
}

.footer {
    width: 100%;
    height: 92rpx;
    position: fixed;
    bottom: 0;
    background-color: #fff;
    z-index: 2;
}

.footer .total {
    position: absolute;
    left: 0;
    display: inline-block;
    width: 390rpx;
    height: 92rpx;
    line-height: 92rpx;
    border-top: 1rpx solid #f7f7f7;
    text-indent: 10rpx;
}

.total .total_text {
    color: #202020;
    font-size: 26rpx;
}

.total .total_price {
    color: #e61f18;
    font-size: 30rpx;
}

.back {
    height: 92rpx;
    width: 140rpx;
    background-color: #f99f36;
    color: #fffefe;
    font-size: 24rpx;
    text-align: center;
    position: absolute;
    right: 220rpx;
    line-height: 45rpx;
}

.back image {
    width: 20rpx;
    height: 34rpx;
    display: block;
    margin: auto;
    margin-top: 12rpx;
}

.back .cartImg {
    width: 40rpx;
    display: block;
    margin: auto;
    margin-top: 12rpx;
}

.pay {
    width: 220rpx;
    height: 92rpx;
    background-color: #e61f18;
    text-align: center;
    line-height: 92rpx;
    /* display: inline-block; */
    float: right;
    color: #fffefe;
    font-size: 28rpx;
}

.mask {
    background: rgba(0, 0, 0, .8);
    width: 100%;
    height: 100%;
    position: fixed;
    left: 0;
    top: 0;
    z-index: 9992;
    display: flex;
    justify-content: center;
    align-items: center;
}

.addres_pop {
    height: 678rpx;
    width: 600rpx;
    opacity: 1;
    background-color: #fff;
    position: relative;
    z-index: 9995;
}

.add_addres {
    width: 600rpx;
    height: 92rpx;
    background-color: #e61f18;
    color: #fff;
    font-size: 28rpx;
    text-align: center;
    line-height: 92rpx;
    position: absolute;
    bottom: 0;
    z-index: 9996;
}

radio {
    transform: scale(0.8);
}

.addres_list {
    width: 100%;
    height: 586rpx;
}

.one_addres {
    width: 544rpx;
    border-bottom: 1rpx solid #f7f7f7;
    padding: 24rpx 28rpx 22rpx 28rpx;
}

.name_phone_detail {
    width: 472rpx;
    display: inline-block;
}

.name_phone_detail .name_phone .name,
.phone {
    font-size: 30rpx;
    color: #404040;
}

.name_phone_detail .addres_detail {
    font-size: 26rpx;
    color: #888;
    line-height: 45rpx;
}

radio-group {
    /* width: 34rpx; */
    height: 100%;
    /* display: inline-block; */
    /* vertical-align: top; */
    float: right;
    margin-top: 5%;
}

::-webkit-scrollbar {
    width: 0rpx;
    height: 0rpx;
    color: transparent;
}

.scroll {
    width: 100%;
    height: 100%;
}

.uncheck {
    width: 32rpx;
    height: 32rpx;
    border: 1px solid #b6b6b6;
}

.s-name {
    width: 100%;
    height: 70rpx;
    line-height: 70rpx;
    font-size: 30rpx;
    background-color: #fff;
    overflow: hidden;
}

.icon-img {
    display: inline-block;
    margin: 20rpx 10rpx 18rpx 20rpx;
    width: 34rpx;
    height: 34rpx;
}

.ad-hint {
    width: 700rpx;
    text-align: center;
    font-size: 30rpx;
    color: #202020;
}

.nullad {
    height: 100rpx;
}

.hint-popup {
    position: fixed;
    display: flex;
    align-items: center;
    justify-content: center;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    overflow: hidden;
    z-index: 10;
}

.fill {
    position: absolute;
    top: 0;
    left: 0;
    width: 100%;
    height: 100%;
    background-color: rgba(0, 0, 0, .7);
    z-index: -1;
}

.hp-cen {
    width: 600rpx;
    background-color: #ffffff;
    border-radius: 2rpx;
}

.b_xie {
    width: 140rpx;
    height: 140rpx;
    margin: -70rpx auto 40rpx;
    background-color: #e61f18;
    border-radius: 50%;
    overflow: hidden;
}

.b-image {
    width: 140rpx;
    height: 140rpx;
}

.hp-info {
    width: 530rpx;
    margin-left: 35rpx;
    line-height: 70rpx;
    font-size: 30rpx;
    color: #404040;
}

.hp-info-moblie {
    margin-left: 25rpx;
}

.hp-address {
    width: 530rpx;
    line-height: 45rpx;
    margin-left: 35rpx;
    font-size: 26rpx;
    color: #888888;
}

.hb-address {
    width: 530rpx;
    line-height: 45rpx;
    text-align: center;
    margin-left: 35rpx;
    font-size: 32rpx;
    color: #404040;
}

.hp-else,
.hp-confirm {
    width: 270rpx;
    height: 80rpx;
    margin: 40rpx 0 60rpx 20rpx;
    border-radius: 2rpx;
    font-size: 28rpx;
    text-align: center;
}

.hp-else {
    box-sizing: border-box;
    border: solid 1rpx #666;
    color: #666;
    background-color: #fff;
}

.hp-confirm {
    background-color: #e61f18;
    color: #fff;
}

.cobuy {
    position: fixed;
    top: 0;
    left: 0;
