<!--pages/boss/earnings/earnings.wxml-->
<view class="top">
    <view class="top-left {{num == 1 ? 'active':''}}" bindtap="clickSelect" data-num="1">个人消费</view>
    <view class="top-left {{num == 2 ? 'active' : ''}}" bindtap="clickSelect" data-num="2">会员圈消费</view>
    <i class="line"><s></s></i>
</view>
<view class="title" wx:if="{{eranList.length > 0 || huiyuan.length > 0}}">收益明细</view>
<view class="list" wx:for="{{eranList}}" wx:if="{{eranList.length > 0 && num == 1}}">
    <view class="list-one" wx:for="{{item}}">
        <view class="touxiang">
            <image src="item.avatar"></image>
        </view>
        <view class="one-info">
            <view class="one-title">{{item.nick_name}}
                <text>{{item.create_time}}</text>
            </view>
            <view class="shouyi">订单收益:
                <text>{{item.result_price}}</text>
            </view>
            <navigator url="/pages/order/orderDetail/orderDetail?orderno={{item.order_on}}">
                <view class="order" >订单号:
                    <text>{{item.order_on}}</text>
                    <image src="{{imgHost}}jt_18.png"></image>
                </view>
            </navigator>
        </view>
    </view>
</view>
<view class="list" wx:for="{{huiyuan}}" wx:if="{{huiyuan.length > 0 && num == 2}}">
    <view class="list-one" wx:for="{{item}}">
        <view class="touxiang">
            <image src="item.avatar"></image>
        </view>
        <view class="one-info">
            <view class="one-title">{{item.nick_name}}
                <text>{{item.create_time}}</text>
            </view>
            <view class="shouyi">合伙人赚取:
                <text>{{item.result_price}}</text>
            </view>
        </view>
    </view>
</view>
<view class="not" wx:if="{{eranList.length <= 0 || huiyuan.length <= 0}}">
    <view class="not-case">
        <image lazy-load="true" src="{{imgHost}}yhq.png"></image>
        <view class="text">暂无明细</view>
    </view>
</view>