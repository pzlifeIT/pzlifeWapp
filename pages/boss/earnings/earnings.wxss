/* pages/boss/earnings/earnings.wxss */
page{
    width: 100%;
    height: 100%;
    background-color: #F7F7F7;
}
.title{
    color: #202020;
    font-size: 32rpx;
    text-align: center;
    width: 750rpx;
    height: 72rpx;
    line-height: 72rpx;
}
.list{
    width: 750rpx;
    height: auto;
    overflow: auto;
}
.list-one{
    width: 750rpx;
    height: 160rpx;
    background-color: #fff;
    margin-bottom: 1rpx;
    display: flex;
    -webkit-display:flex;
    align-items: center;
}
.touxiang{
    height: 100%;
    width: 134rpx;
    box-sizing: border-box;
    text-align: center;
    display: flex;
    justify-content: center;
    align-items: center;
}
.touxiang image{
    width: 96rpx;
    height: 96rpx;
    border-radius: 50%;
    background-color: #fdedb5;
}
.one-info{
    height: 100%;
    width: auto;
}
.one-title{
    font-size:28rpx ;
    color: #202020;
    margin-top: 23rpx;
    margin-bottom: 8rpx;
}
.one-title text{
    font-size: 28rpx;
    color: #404040;
}
.order,.shouyi{
    color: #666666;
    font-size: 26rpx;
    margin-bottom: 8rpx;
}
.order text,.shouyi text{
    color: #202020;
}
