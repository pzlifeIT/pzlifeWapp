/* pages/boss/boss.wxss */
page{
	background: #F7F7F7;
}
.background{
	width: 750rpx;
	height: 319rpx;
	background: url(data:image/png;base64,);
	background-repeat: no-repeat;
	background-size: 750rpx 319rpx;
	overflow: hidden;
	position: relative;
}
.background .sz{
	width: 43rpx;
	height: 43rpx;
	position: absolute;
	right: 20rpx;
	top: 33rpx;
}
.boss-info{
	width: 500rpx;
	height: 124rpx;
	display: flex;
	/* justify-content: center; */
	align-items: center;
	margin-top: 44rpx;
	margin-left: 30rpx;
}
.head-img{
	width: 116rpx;
	height: 116rpx;
	display: inline-block;
}
.head-img image{
	width: 100%;
	height: 100%;
	background: #AFEEEE;
	border-radius: 50%;
}
.nickname{
	display: inline-block;
	margin-left: 8rpx;
}
.name{
	font-size: 32rpx;
	color: #fff;
	display: inline-block;
}
.identity{
	background: #fff;
	text-align: center;
	color: #E61F18;
	font-size: 22rpx;
	border-radius: 2rpx;
	display: inline-block;
	padding: 0 10rpx;
	margin-left: 10rpx ;
}
.shop-id{
	font-size: 26rpx;
	color: #fff;
	margin-top: 10rpx;
}
.statistics{
	width: 710rpx;
	/* height: 140rpx; */
	overflow: hidden;
	margin: 0 auto;
	position: relative;
	top: -99rpx;
	z-index: 99;
}
.earnings{
	width: 710rpx;
	height: 140rpx;
	background: #fff;
	margin-bottom: 10rpx;
}
.div{
	width: 33.33%;
	height: 140rpx;
	display: inline-block;
	text-align: center;
	box-sizing: border-box;
}
.after::after{
	width: 1px;
	height: 70rpx;
	background: red;
	/* position: relative; */
	z-index: 9999;
}
.div .title{
	color: #404040;
	font-size: 24rpx;
	margin-bottom: 20rpx;
	margin-top: 26rpx;
}
.div .num{
	color: #E61F18;
	font-size: 30rpx;
}
.num text{
	color: #404040;
	font-size: 24rpx;
	margin-left: 4rpx;
	display: inline-block;
}
.icon{
	width: 710rpx;
	height: 342rpx;
}
.one{
	width: 33.33%;
	height: 170rpx;
	text-align: center;
	float: left;
	background: #fff;
	margin-bottom: 1rpx;
	box-sizing: border-box;
	border-bottom: 1rpx solid #F7F7F7;
}
.icon-after{
	border-right: 1rpx solid #F7F7F7;
}
.one image{
	width: 76rpx;
	height: 76rpx;
	margin-top: 24rpx;
}
.icon-text{
	font-size: 26rpx;
	color: #404040;
}





