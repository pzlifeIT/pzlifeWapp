page{
    background: #F7F7F7;
    height: 100%;
}
.card-list{
    width: 750rpx;
    height: auto;
    /*overflow: auto;*/
    display: flex;
    flex-direction: column;
    align-items: center;
    background: #F7F7F7;
    margin-bottom: 100rpx;
}
.kefu{
    width: 710rpx;
    height: 66rpx;
    text-align: center;
    line-height: 66rpx;
}
.dian{
    width: 10rpx;
    height: 10rpx;
    background: #E61F18;
    display: inline-block;
    border-radius: 50%;
    margin-right: 9rpx;
    margin-bottom: 5rpx;
}
.phone{
    color: #404040;
    font-size: 26rpx;
    display: inline-block;
}
.one-card{
    width: 710rpx;
    height: 230rpx;
    /*background: #FFFFFF;*/
    border-radius: 10rpx;
    position: relative;
    box-shadow: 0px 2px 6px 0px rgba(0, 0, 0, 0.15);
    margin-bottom: 20rpx;
}
.bg{
    width: 100%;
    height: 100%;
    position: absolute;
    top: 0;
    left: 0;
    border-radius: 10rpx;
    /*z-index: -1;*/
}
.logo-name{
    width: auto;
    height: auto;
    display: flex;
    align-items: center;
    justify-content: flex-start;
    margin:20rpx 0 0 20rpx;
    position: absolute;
    z-index: 2;
}
.logo{
    width: 80rpx;
    height: 80rpx;
    border-radius: 50%;
    margin-right: 20rpx;
}
.yh-name{
    color: #202020;
    font-size: 34rpx;
    display: inline-block;
}
.cardNum{
    font-size: 36rpx;
    color: #202020;
    display: inline-block;
    position: absolute;
    left: 30rpx;
    bottom: 30rpx;
}
.button{
    width: 150rpx;
    height: 52rpx;
    border-radius: 26rpx;
    text-align: center;
    line-height: 52rpx;
    color: #FFFFFF;
    font-size: 28rpx;
    position: absolute;
    right: 20rpx;
    bottom: 27rpx;
    z-index: 1;
    background: #E61F18;
}
.un-dispose{
    background: #f99f36;
    bottom: 109rpx;
}
.reason{
    color: #404040;
    font-size: 26rpx;
    width: 129rpx;
    position: absolute;
    bottom: 40rpx;
    right: 20rpx;
}
.jt{
    width: 12rpx;
    height: 21rpx;
}
.ty{
    background: #E61F18;
    padding-left: 48rpx;
    box-sizing: border-box;
}
.qy{
    background: #18c745;
    padding-right: 48rpx;
    box-sizing: border-box;
}
.yuan{
    width: 50rpx;
    height: 50rpx;
    border-radius: 50%;
    position: absolute;
    top: 0rpx;
    background: #FFFFFF;
}
.ty-yuan{
    left: 0rpx;
}
.qy-yuan{
    right: 0rpx;
}
.footer{
    width: 460rpx;
    height: 80rpx;
    background: #fff;
    border: 1px solid #e61f18;
    border-radius: 15rpx;
    box-shadow: 3rpx 4rpx 6rpx rgba(0,0,0,0.16);
    font-size: 32rpx;
    color: #E61F18;
    text-align: center;
    line-height: 80rpx;
    position: fixed;
    bottom: 300rpx;
    left: 145rpx;
    z-index: 99;
}
.null{
    width: 750rpx;
    height: 100%;
    display: flex;
    justify-content: center;
    align-items: center;
}
.center{
    width: 212rpx;
    height: auto;
    overflow: auto;
}
.center image{
    width: 210rpx;
    height: 140rpx;
    margin-bottom: 48rpx;
}
.null-text{
    color: #d6d6d6;
    font-size: 28rpx;
    width: 100%;
    text-align: center;
}
.have{
    bottom: 10rpx;
}