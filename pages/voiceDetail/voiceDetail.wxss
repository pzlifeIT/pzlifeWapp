}

.desc .text {
    width: 510rpx;
    float: left;
    margin: 20rpx;
}

.putaway {
    width: 260rpx;
    height: 62rpx;
    font-size: 28rpx;
    margin-top: 20rpx;
    margin-right: 20rpx;
    background-color: #e61f18;
    border-radius: 6px;
    color: #fff;
    text-align: right;
}

.putaway i {
    position: relative;
    display: inline-block;
    box-sizing: border-box;
    width: 36rpx;
    height: 36rpx;
    margin: 13rpx 0 13rpx 11rpx;
    border: 2rpx solid #fff;
    border-radius: 50%;
}

.putaway i::before,
.putaway i::after {
    position: absolute;
    display: block;
    content: "";
    background-color: #fff;
}

.putaway i::after {
    top: 8rpx;
    left: 16rpx;
    width: 2rpx;
    height: 20rpx;
}

.putaway i::before {
    top: 16rpx;
    left: 8rpx;
    width: 20rpx;
    height: 2rpx;
}

.putaway text {
    display: inline-block;
    line-height: 62rpx;
    margin-right: 10rpx;
}

.desc text {
    display: block;
}

.desc .title {
    font-size: 36rpx;
    color: #101010;
    margin-bottom: 17rpx;
    max-width: 510rpx;
    overflow: hidden;
    text-overflow: ellipsis;
    display: -webkit-box;
    -webkit-box-orient: vertical;
    -webkit-line-clamp:2;
}

.desc .subtitle {
    font-size: 30rpx;
    color: #b6b6b6;
    margin-bottom: 31rpx;
}

.desc .price {
    width: 400rpx;
