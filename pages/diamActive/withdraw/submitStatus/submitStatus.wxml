<!--pages/boss/withdraw/submitStatus/submitStatus.wxml-->
<view class="status">
    <view class="img">
        <image lazy-load="true" src="{{imgHost}}lb.png"></image>
        <text>申请提现成功</text>
    </view>
    <view class="jg">请耐心等待财务进行操作</view>
</view>

<view class="footer">
