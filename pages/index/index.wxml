<view class="in-slider">
	<view class="header">
		<navigator hover-class="none" url='/pages/search/search' hover-class="none">
			<view class="index-search din fl">
				<icon type="search" size="34rpx" color="#fff" />
				<input class="search-inp din fr" type="text" placeholder-class="pla" placeholder="输入您当前要搜索的商品" disabled='disabled' />
			</view>
		</navigator>
	</view>
	<swiper class="slider" autoplay="true" current="0" circular="true" bindchange="swiper">
		<swiper-item class="pz-swiper" wx:for="{{slider.son}}" wx:key="index">
			<navigator hover-class="none" wx:if="{{item.jump_type == 1}}" url="/pages/category/goods/goods?sub_id={{item.jump_content}}" open-type="navigate">
				<image lazy-load="true" src="{{item.image_path+'?imageMogr2/thumbnail/370x'}}"></image>
			</navigator>
			<navigator hover-class="none" wx:elif="{{item.jump_type == 2}}" url="/pages/goods/detail?goodid={{item.jump_content}}" open-type="navigate">
				<image lazy-load="true" src="{{item.image_path+'?imageMogr2/thumbnail/370x'}}"></image>
			</navigator>
			<navigator hover-class="none" wx:elif="{{item.jump_type == 3}}" url="{{item.jump_content}}" open-type="navigate">
				<image lazy-load="true" src="{{item.image_path+'?imageMogr2/thumbnail/370x'}}"></image>
			</navigator>
		</swiper-item>
	</swiper>
	<view class="num">{{num}}/{{slider.son.length}}</view>
</view>
<view class="icon">
	<view class="icon-line">

		<view class="icon-one" wx:for="{{icon.son}}">
			<navigator hover-class="none" wx:if="{{item.jump_type == 1}}" url="/pages/category/goods/goods?sub_id={{item.jump_content}}" open-type="navigate">
				<image lazy-load="true" src="{{item.image_path+'?imageMogr2/thumbnail/370x'}}"></image>
				<view class="icon-text">{{item.title}}</view>
			</navigator>
			<navigator hover-class="none" wx:if="{{item.jump_type == 3}}" url="{{item.jump_content}}" open-type="reLaunch">
				<image lazy-load="true" src="{{item.image_path+'?imageMogr2/thumbnail/370x'}}"></image>
				<view class="icon-text">{{item.title}}</view>
			</navigator>
			<navigator hover-class="none" wx:if="{{item.jump_type == 2}}" url="/pages/goods/detail?goodid={{item.jump_content}}" open-type="navigate">
				<image lazy-load="true" src="{{item.image_path+'?imageMogr2/thumbnail/370x'}}"></image>
				<view class="icon-text">{{item.title}}</view>
