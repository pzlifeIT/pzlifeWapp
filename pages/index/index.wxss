    margin-bottom: 10rpx;
}

.line {
    position: absolute;
    bottom: 0;
    width: 33.33%;
    height: 2rpx;
    /* transition: all 0.3 ease-in-out; */
}

.line s {
    width: 100%;
    height: 2rpx;
    background: #E61F18;
