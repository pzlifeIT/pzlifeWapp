page {
    background: #F7F7F7;
    width: 100%;
    height: 100%;
}

.header {
    width: 100%;
    overflow: auto;
    position: absolute;
    top: 0;
    z-index: 88;
}

.in-slider {
    width: 750rpx;
    overflow: hidden;
    position: relative;
}

.logo {
    width: 108rpx;
    height: 61rpx;
    margin: 20rpx 0;
}

.index-search {
    width: 710rpx;
    /* height: 60rpx; */
    margin: 20rpx 0;
    margin-left: 20rpx;
    border-radius: 10rpx;
    overflow: hidden;
    border-radius: 30rpx;
    position: relative;
}

.index-search icon {
    margin: 13rpx 0 13rpx 21rpx;
    position: absolute;
    left: 190rpx;
}

.index-search .search-inp {
    width: 710rpx;
    height: 60rpx;
    line-height: 60rpx;
    font-size: 26rpx;
    background-color: rgba(0, 0, 0, .3);
    outline: none;
    border: none;
    border-left: none;
    text-align: center;
    color: #fff;
}

.pla {
    color: #fff;
    margin-left: 40rpx;
}

.slider {
    width: 750rpx;
    height: 456rpx;
}

.pz-swiper {
    width: 750rpx;
    height: 456rpx;
}

.pz-swiper image {
    width: 100%;
    height: 456rpx;
}

.num {
    width: 64rpx;
    height: 32rpx;
    background: rgba(0, 0, 0, 0.47);
    border-radius: 15rpx;
    color: #fff;
    font-size: 26rpx;
    position: absolute;
    bottom: 18rpx;
    right: 40rpx;
    text-align: center;
    line-height: 32rpx;
}

.icon {
    width: 750rpx;
    height: 332rpx;
    overflow: hidden;
    padding-top: 32rpx;
    background: #fff;
}

.icon-line {
    width: 100%;
    height: 50%;
}

.icon-one {
    width: 20%;
    height: 100%;
    text-align: center;
    float: left;
}

.icon-one image {
    width: 88rpx;
    height: 88rpx;
}

.icon-text {
    color: #404040;
    font-size: 28rpx;
    width: 100%;
    text-align: center;
}

.double-div {
    width: 750rpx;
    height: 185rpx;
    display: flex;
    justify-content: center;
    align-items: center;
}

.div-left,
.div-right {
    width: 350rpx;
    height: 145rpx;
    float: left;
}

.div-left {
    margin-right: 10rpx;
}

.div-left image,
.div-right image {
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
}

.newpro {
    width: 750rpx;
    position: relative;
    background: url(data:image/png;base64,);
    background-size: 750rpx 257rpx;
    background-repeat: no-repeat;
    overflow: hidden;
    background-color: #fff;
}

.new-title {
    width: 750rpx;
    height: 257rpx;
    position: absolute;
    top: 0;
}

.new-one {
    width: 710rpx;
    height: 240rpx;
    border-radius: 20rpx;
    margin: 0 auto;
}

.new-one image {
    width: 100%;
    height: 100%;
    border-radius: 20rpx;
}

.title {
    width: 710rpx;
    height: 80rpx;
    /* margin: 25rpx 0 23rpx 0; */
    line-height: 80rpx;
    padding: 0 20rpx 0 20rpx;
}

.newpro-text {
    font-size: 32rpx;
    color: #202020;
    font-weight: bold;
    float: left;
}

.new-more {
    font-size: 28rpx;
    color: #404040;
    float: right;
}

.new-more image {
    width: 14rpx;
    height: 24rpx;
}

.new-line {
    width: 710rpx;
    height: 245rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    margin: 0 auto;
}

.new-mid {
    margin: 0 10rpx;
}

.new-two,
.new-two image {
    width: 230rpx;
    height: 215rpx;
    border-radius: 20rpx;
}

.date-banner {
    width: 750rpx;
    height: 157rpx;
    margin: 20rpx 0;
    display: flex;
    justify-content: center;
    align-items: center;
    background-color: #fff;
}

.date-banner image {
    width: 710rpx;
    height: 127rpx;
    margin: 0 atuo;
}

.goods-content {
