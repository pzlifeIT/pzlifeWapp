    white-space: nowrap;
    text-overflow: ellipsis;
}

.goods-price {
    color: #E61F18;
    font-size: 30rpx;
}

.vipprice {
    color: #eab700;
    font-size: 24rpx;
}

.life-recommend {
    width: 750rpx;
    overflow: hidden;
    white-space: nowrap;
    position: relative;
}

.life-title {
    width: 750rpx;
    height: 146rpx;
    background: -webkit-gradient(linear, center top, center bottom, from(#F7F7F7), to(#FFFFFF));
    padding-top: 20rpx;
}

.life-title image {
    width: 750rpx;
    height: 146rpx;
}

.life-goods {
